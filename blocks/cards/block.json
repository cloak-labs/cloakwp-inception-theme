{
  "name": "acf/cards",
  "title": "Cards",
  "description": "Display a few cards in a grid.",
  "textdomain": "cloakwp",
  "category": "widgets",
  "icon": "admin-page",
  "keywords": [
    "cards",
    "posts",
    "pages",
    "tiles"
  ],
  "acf": {
    "mode": "edit"
  },
  "align": false,
  "supports": {
    "anchor": true,
    "mode": true,
    "align": false,
    "html": false,
    "reusable": false,
    "lock": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    },
    "color": {
      "background": true,
      "gradients": false,
      "text": false
    }
  },
  "attributes": {
    "backgroundColor": {
      "type": "string",
      "default": "gray-100"
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "margin": {
            "top": "72px",
            "bottom": "72px"
          }
        }
      }
    }
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "cloakwp_block_inserter_preview_image": "/blocks/cards/screenshot.png"
      }
    }
  }
}