{
  "$schema": "https://advancedcustomfields.com/schemas/json/main/block.json",
  "name": "contact-form",
  "title": "Contact Form",
  "description": "A generic contact form.",
  "textdomain": "cloakwp",
  "category": "widgets",
  "icon": "feedback",
  "keywords": [
    "contact",
    "form"
  ],
  "acf": {
    "mode": "preview",
    "postTypes": [
      "page"
    ]
  },
  "supports": {
    "jsx": true,
    "anchor": true,
    "mode": true,
    "align": true,
    "html": false,
    "reusable": false,
    "lock": false,
    "multiple": false,
    "color": {
      "background": true,
      "gradients": false,
      "text": false
    },
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    }
  },
  "attributes": {
    "backgroundColor": {
      "type": "string",
      "default": "bg-root"
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|20",
            "bottom": "var:preset|spacing|60"
          }
        }
      }
    }
  }
}