{
  "$schema": "https://advancedcustomfields.com/schemas/json/main/block.json",
  "name": "testimonial",
  "title": "Testimonial(s)",
  "description": "Display testimonials.",
  "textdomain": "cloakwp",
  "category": "widgets",
  "icon": "editor-quote",
  "keywords": [
    "testimonial",
    "review",
    "client",
    "customer",
    "quote"
  ],
  "acf": {
    "mode": "edit"
  },
  "supports": {
    "anchor": true,
    "mode": true,
    "align": [
      "wide",
      "full"
    ],
    "html": false,
    "reusable": false,
    "lock": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    }
  },
  "attributes": {
    "align": {
      "type": "object",
      "default": "wide"
    },
    "style": {
      "type": "object",
      "default": {
        "spacing": {
          "margin": {
            "top": "var:preset|spacing|70",
            "bottom": "var:preset|spacing|70"
          }
        }
      }
    }
  }
}