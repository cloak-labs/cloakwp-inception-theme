{
  "$schema": "https://advancedcustomfields.com/schemas/json/main/block.json",
  "name": "posts",
  "title": "Posts",
  "description": "Display an archive of posts (or CPTs).",
  "textdomain": "cloakwp",
  "category": "widgets",
  "icon": "admin-page",
  "keywords": [
    "posts",
    "pages",
    "query",
    "tiles",
    "list"
  ],
  "acf": {
    "mode": "edit"
  },
  "supports": {
    "anchor": true,
    "mode": true,
    "align": [
      "wide"
    ],
    "html": false,
    "reusable": false,
    "lock": false,
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    }
  },
  "attributes": {
    "align": {
      "type": "object",
      "default": "wide"
    }
  },
  "example": {
    "attributes": {
      "mode": "preview",
      "data": {
        "cloakwp_block_inserter_iframe": true,
        "field_fd80b38e": "Post",
        "field_41fc116f": "Learn more",
        "field_f0ced213": [
          "date"
        ]
      },
      "style": {
        "spacing": {
          "margin": {
            "top": "0px",
            "bottom": "0px"
          }
        }
      }
    },
    "viewportWidth": 1000
  }
}